{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Emoji Insert",
  "description": "Insert emoticons with the context menu",
  "version": "1.0",
  "permissions": ["contextMenus","tabs","activeTab"],
  "background": {
    "scripts": ["EmojiContextMenu.js"]
  },
  "manifest_version": 2,
  "icons": { 
           "48": "icon48.png",
          "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
